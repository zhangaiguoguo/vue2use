{
  "name": "vue2use",
  "version": "1.0.10",
  "description": "vue2use",
  "main": "main.ts",
  "module": "dist/vue2use.esm.js",
  "types": "types/index.d.ts",
  "unpkg": "dist/vue2use.global.prod.js",
  "jsdelivr": "dist/vue2use.global.prod.js",
  "files": [
    "dist",
    "types",
    "index.js"
  ],
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "node": {
        "production": "./dist/vue2use.cjs.js",
        "development": "./dist/vue2use.cjs.js",
        "default": "./index.js"
      },
      "module": "./dist/vue2use.esm.js",
      "import": "./dist/vue2use.esm.js",
      "require": "./dist/vue2use.cjs.js"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhangaiguoguo/vue2use.git"
  },
  "buildOptions": {
    "name": "vue2use",
    "formats": [
      "esm",
      "cjs",
      "global"
    ]
  },
  "keywords": [
    "vue",
    "vue reactivity",
    "vue2 componentApi",
    "vue2Hooks",
    "vue setup"
  ],
  "author": "zhangag",
  "license": "MIT",
  "bugs": {},
  "devDependencies": {
    "vue": "^2.5.22"
  }
}
